<h3>Log in:</h3>

<%= form_for @user, url: '/login' do |f| %>

<%= f.label :username  %>
<%= f.text_field :username %><br>

<%= f.label :password %>
<%= f.password_field :password %><br>

<%= f.submit "Login" %>

<%= link_to "Login via GitHub", '/auth/github'%>

<h6>Don't have an account? <%=link_to 'Signup', signup_path%></h6>

<%end%>
